{
  "name": "delivery-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://carloscedeno-creator.github.io/delivery-dashboard",
  "scripts": {
    "check-epic-dates": "node scripts/check-epic-dates-in-db.js",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy": "npm run build && gh-pages -d dist",
    "calculate-metrics": "node scripts/calculate-metrics.js",
    "test-metrics": "node scripts/test-metrics-calculation.js",
    "analyze": "node scripts/analyze-supabase.js",
    "query": "node scripts/query-example.js",
    "compare-formats": "node scripts/compare-csv-vs-supabase.js",
    "check-sprint": "node scripts/check-sprint-issues.js",
    "check-status": "node scripts/check-issue-status.js",
    "force-sync-issues": "node scripts/force-sync-issues.js",
    "update-status": "node scripts/update-issue-status.js",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit",
    "test:unit:watch": "vitest watch tests/unit",
    "test:unit:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.87.1",
    "date-fns": "^3.3.1",
    "dotenv": "^17.2.3",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.344.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.12.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^4.0.16",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "vite": "^5.1.6",
    "vitest": "^4.0.16"
  }
}
